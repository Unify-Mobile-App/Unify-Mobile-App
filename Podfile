# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Unify-Mobile-App' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Unify-Mobile-App
  pod 'Observable'
  pod 'Floaty'
  pod 'IQKeyboardManagerSwift'
  pod 'SearchTextField'
  pod 'Nimble'

  target 'Unify-Mobile-AppTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Nimble'
    pod 'ViewControllerPresentationSpy', '~> 6.0'

  end

  target 'Unify-Mobile-AppUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Nimble'
    pod 'ViewControllerPresentationSpy', '~> 6.0'

  end

end
